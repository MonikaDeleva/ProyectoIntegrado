<AnchorPane id="AnchorPane" prefHeight="720.0" prefWidth="1280.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.RegistrarseController">
   <children>
      <Pane layoutX="736.0" layoutY="164.0" prefHeight="502.0" prefWidth="396.0" style="-fx-border-color: black;">
         <children>
            <TextField fx:id="textFieldNombre" layoutX="124.0" layoutY="122.0" onAction="#paraNombre" promptText="Nombre" />
            <TextField fx:id="textFieldApellidos" layoutX="124.0" layoutY="166.0" onAction="#paraApellidos" promptText="Apellidos" />
            <TextField fx:id="textFieldEmail" layoutX="124.0" layoutY="215.0" onAction="#paraEmail" promptText="email" />
            <DatePicker fx:id="datepickerFecha" layoutX="124.0" layoutY="262.0" onAction="#paraFechaNacimiento" prefHeight="26.0" prefWidth="149.0" promptText="Fecha de nacimiento" />
            <PasswordField fx:id="PasswordField" layoutX="124.0" layoutY="312.0" onAction="#paraContraseña" promptText="Contraseña" />
            <Button fx:id="buttonRegistrarse" layoutX="161.0" layoutY="369.0" mnemonicParsing="false" onAction="#paraRegistrarse" text="Registrarse" />
            <Label fx:id="labelRegistrarse" layoutX="113.0" layoutY="38.0" prefHeight="54.0" prefWidth="221.0" text="Registrarse">
